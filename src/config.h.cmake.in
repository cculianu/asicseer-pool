#if (WORDS_BIGENDIAN && WORDS_LITTLEENDIAN) || (!WORDS_BIGENDIAN && !WORDS_LITTLEENDIAN)
#error "Must define at exactly one of: WORDS_BIGENDIAN or WORDS_LITTLEENDIAN"
#endif

/* Name of package */
#define PACKAGE "${APP_NAME}"

/* Define to the address where bug reports for this package should be sent. */
#define PACKAGE_BUGREPORT "${APP_BUGREPORT}"

/* Define to the full name of this package. */
#define PACKAGE_NAME "${APP_NAME}"

/* Define to the full name and version of this package. */
#define PACKAGE_STRING "${APP_NAME} " VERSION

/* Define to the one symbol short name of this package. */
#define PACKAGE_TARNAME "${APP_NAME}"

/* Define to the home page for this package. */
#define PACKAGE_URL "${APP_BUGREPORT}"

/* Define to the version of this package. */
#define PACKAGE_VERSION VERSION

/* If using the C implementation of alloca, define if you know the
   direction of stack growth for your system; otherwise it will be
   automatically deduced at runtime.
    STACK_DIRECTION > 0 => grows toward higher addresses
    STACK_DIRECTION < 0 => grows toward lower addresses
    STACK_DIRECTION = 0 => direction of growth unknown */
/* #undef STACK_DIRECTION */

/* CMakeLists.txt defaults this to on, but can be turned off at the cmake line */
#cmakedefine USE_ASM 1

/* Use avx2 assembly instructions for sha256 */
#cmakedefine USE_AVX2 1

/* Use avx1 assembly instructions for sha256 */
#cmakedefine USE_AVX1 1

/* Use sse4 assembly instructions for sha256 */
#cmakedefine USE_SSE4 1

/* Enable extensions on AIX 3, Interix.  */
#ifndef _ALL_SOURCE
# define _ALL_SOURCE 1
#endif
/* Enable GNU extensions on systems that have them.  */
#ifndef _GNU_SOURCE
# define _GNU_SOURCE 1
#endif
/* Enable threading extensions on Solaris.  */
#ifndef _POSIX_PTHREAD_SEMANTICS
# define _POSIX_PTHREAD_SEMANTICS 1
#endif
/* Enable extensions on HP NonStop.  */
#ifndef _TANDEM_SOURCE
# define _TANDEM_SOURCE 1
#endif
/* Enable general extensions on Solaris.  */
#ifndef __EXTENSIONS__
# define __EXTENSIONS__ 1
#endif

/* Define to 1 if on MINIX. */
#cmakedefine _MINIX 1

/* Define to 2 if the system does not provide POSIX.1 features except with
   this defined. */
#cmakedefine _POSIX_1_SOURCE 1

/* Define to 1 if you need to in order for `stat' and other things to work. */
#cmakedefine _POSIX_SOURCE 1

/* Define to `unsigned int' if <sys/types.h> does not define (very old unix). */
#cmakedefine size_t ${size_t}

/* For the various cpu-specific bits in bitcoin/sha256_*.cpp */
#cmakedefine ENABLE_SSE41 1
#cmakedefine ENABLE_AVX2 1
#cmakedefine ENABLE_SHANI 1

#endif /* HAVE_CONFIG_H */
